{
  "my_count":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/master/my-count.html",
      "description":"Returns number of documents across all indices or within a specified set of indices."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"],
      "content_type": ["application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_my_count",
          "methods":[
            "GET"
          ]
        },
        {
          "path":"/_my_count/{name}",
          "methods":[
            "GET"
          ],
          "parts":{
            "name":{
              "type":"list",
              "description":"A comma-separated list of indices to restrict the results"
            }
          }
        }
      ]
    },
    "params":{
      "expand_wildcards":{
        "type":"enum",
        "options":[
          "open",
          "closed",
          "hidden",
          "none",
          "all"
        ],
        "default":"open",
        "description":"Whether to expand wildcard expression to concrete indices that are open, closed or both."
      }
    }
  }
}
